{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "dist", "documentation", "test"],
    "includePattern": "\\.(js|ts)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component",
    "better-docs/typescript"
  ],
  "templates": {
    "better-docs": {
      "name": "Blog Api"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
